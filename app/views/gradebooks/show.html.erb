<div>
  <h3><%= link_to "Department", departments_path -%> >> <%= @department.name -%><h3>
  <div id="dpt_header" class="clearfix">
    <h4>Programme</h4>
    <%= link_to "+ Add Program", new_program_path(:department_id => @department), :class => "alr button" -%>
  </div>
  <ul id="departments">
    <% @gradebook.each do |p| -%>
    <li class="clearfix">
	    <%= link_to p.marks, course_path(p, :course_session_id => @course_session_id) -%>
	    <div class="edit-holder alr">
	      <ul class="edit-box">
	        <li>
	          <%= link_to "Edit",update_gradebook_path(p,  :course_session_id => @course_session_id), :class => "edit-but" -%>
	          <ul class="edit-sub">
		          <li> <%= link_to "Edit",update_gradebook_path(p,  :course_session_id => @course_session_id) -%></li>
		        
	          </ul>
	        </li>
	      </ul>
	    </div>
    </li>
    <% end %>
  </ul>
</div>
